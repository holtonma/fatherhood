<h1>Listing temperature_types</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Note</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @temperature_types.each do |temperature_type| %>
  <tr>
    <td><%= temperature_type.name %></td>
    <td><%= temperature_type.note_id %></td>
    <td><%= link_to 'Show', temperature_type %></td>
    <td><%= link_to 'Edit', edit_temperature_type_path(temperature_type) %></td>
    <td><%= link_to 'Destroy', temperature_type, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

