<h1>Listing feeding_types</h1>

<table>
  <tr>
    <th>ID</th>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @feeding_types.each do |feeding_type| %>
  <tr>
    <td><%= feeding_type.id %></td>
    <td><%= feeding_type.name %></td>
    <td><%= link_to 'Show', feeding_type %></td>
    <td><%= link_to 'Edit', edit_feeding_type_path(feeding_type) %></td>
    <td><%= link_to 'Destroy', feeding_type, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

